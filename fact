PL-cLab:
fact(N: int): int
START
  inc(N);
  I: int;
  inc(I);
  RES: int;
  inc(RES);
  FROM 1 TO N WITH 1:
  START
    assign(RES, mul(RES, I));
  inc(I);
  FINISH
  RETURN RES;
FINISH
MAIN
print(fact(10));

